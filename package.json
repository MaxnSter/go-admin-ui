{
  "name": "go-admin",
  "version": "3.0.0",
  "description": "A magical vue admin. An out-of-box UI solution for enterprise applications. Newest development stack of vue. Lots of awesome features",
  "author": "https://github.com/wenjianzhang",
  "license": "MIT",
  "packageManager": "pnpm@8.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:prod": "vue-tsc && vite build --mode production",
    "build:stage": "vue-tsc && vite build --mode staging",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "lint:module": "eslint src --fix",
    "lint:ui": "eslint src/components/**/*.vue src/views/**/*.vue --fix",
    "test:ui": "vitest run tests/unit/components",
    "type-check": "vue-tsc --noEmit",
    "verify:syntax": "node scripts/check-vue3-syntax.js",
    "verify:imports": "node scripts/check-imports.js",
    "verify:filters": "node scripts/check-filters.js",
    "verify:directives": "node scripts/check-directives.js",
    "verify:dependencies": "node scripts/check-dependencies.js",
    "verify:build-dev": "vite build --mode development",
    "verify:build-prod": "vite build --mode production",
    "verify:dev-server": "timeout 30s pnpm dev || echo 'Dev server check completed'",
    "verify:bundle-size": "vite build && du -sh dist",
    "build:check": "vite build",
    "verify:task3": "pnpm lint:ui && pnpm test:ui && pnpm type-check && pnpm build:check",
    "test:unit": "vitest",
    "test:router": "vitest run tests/unit/router",
    "test:directives": "vitest run tests/unit/directives",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "admin",
    "go-admin",
    "go-admin-ui",
    "dashboard",
    "element-ui",
    "boilerplate",
    "admin-template",
    "management-system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/go-admin-team/go-admin.git"
  },
  "bugs": {
    "url": "https://github.com/go-admin-team/go-admin/issues"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@riophae/vue-treeselect": "^0.4.0",
    "axios": "^1.6.2",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.10",
    "element-plus": "^2.4.4",
    "file-saver": "^2.0.5",
    "fuse.js": "^7.0.0",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "@codemirror/vue": "^6.0.1",
    "vue-echarts": "^7.0.0",
    "vue-advanced-cropper": "^2.8.0",
    "vue-draggable-plus": "^2.1.0",
    "screenfull": "^6.0.2",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "glob": "^11.0.2",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "typescript": "~5.3.3",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
